{
  "name": "@amalv/storybook-design-system",
  "version": "0.1.41",
  "private": false,
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "https://github.com/amalv/storybook-design-system.git",
  "dependencies": {
    "polished": "^3.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "3.2.0",
    "styled-components": "^4.4.0"
  },
  "scripts": {
    "build": "build-storybook -o build && npm run build-package",
    "release": "auto shipit",
    "deploy-storybook": "storybook-to-ghpages",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-storybook": "build-storybook -s public",
    "build-storybook-docs": "build-storybook -s public --docs",
    "storybook": "start-storybook -p 9009 -s public",
    "build-package": "BABEL_ENV=production babel src -d dist",
    "chromatic": "chromatic"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "@babel/plugin-transform-modules-commonjs"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@storybook/addon-a11y": "^5.2.5",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-docs": "^5.2.5",
    "@storybook/addon-knobs": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addon-storysource": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@storybook/storybook-deployer": "^2.8.1",
    "auto": "^7.12.4",
    "prettier": "^1.18.2",
    "storybook-chromatic": "^3.0.3"
  },
  "description": "Learn more at [Learn Storybook](https://learnstorybook.com).",
  "bugs": {
    "url": "https://github.com/amalv/storybook-design-system/issues"
  },
  "homepage": "https://github.com/amalv/storybook-design-system#readme",
  "author": "Amaro Mari√±o"
}
